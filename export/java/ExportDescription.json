{
    "Defaults": {
	"All": {
	    "Import": "import edu.wpi.first.wpilibj.${ClassName};",
	    "Declaration": "public static ${ClassName} ${Name};",
	    "Extra": ""
	},
	"None": {
	    "Import": "",
	    "Declaration": "",
	    "Construction": "",
	    "Extra": ""
	},
	"AnalogInput": {
	    "Construction": "${Name} = new ${ClassName}(${Input Module (Analog)}, ${Input Port (Analog)});"
	},
	"DigitalInput": {
	    "Construction": "${Name} = new ${ClassName}(${Input Module (Digital)}, ${Input Port (Digital)});"
	},
	"PWMOutput": {
	    "Construction": "${Name} = new ${ClassName}(${Output Module (Digital)}, ${Output Port (PWM)});"
	}
    },
    "Palette": {
	"Robot" : {
	    "Defaults": ["None"],
	    "ClassName": ""
	},
	"Subsystem" : {
	    "Defaults": ["None"],
	    "ClassName": ""
	},
	"PID Controller" : {
	    "Defaults": ["All"],
	    "ClassName": "PIDController",
	    "Construction": "${Name} = new ${ClassName}(${P}, ${I}, ${D}, ${F}, ${Input}, ${Output}, ${Period});"
	},
	"Robot Drive 2" : {
	    "Defaults": ["All"],
	    "ClassName": "RobotDrive",
	    "Construction": "${Name} = new ${ClassName}(${Left Motor}, ${Right Motor});"
	},
	"Robot Drive 4" : {
	    "Defaults": ["All"],
	    "ClassName": "RobotDrive",
	    "Construction": "${Name} = new ${ClassName}(${Left Front Motor}, ${Left Rear Motor}, ${Right Front Motor}, ${Right Rear Motor});"
	},
        "Gyro" : {
	    "Defaults": ["AnalogInput", "All"],
	    "ClassName": "Gyro",
	    "Extra": "${Name}.setSensitivity(${Sensitivity});"
        },
        "Accelerometer" : {
	    "Defaults": ["AnalogInput", "All"],
	    "ClassName": "Accelerometer"
        },
        "Quadrature Encoder" : {
	    "Defaults": ["DigitalInput", "All"],
	    "ClassName": "Encoder",
	    "Construction": "${Name} = new ${ClassName}(${A Channel Module (Digital)}, ${A Channel Port (Digital)}, ${B Channel Module (Digital)}, ${B Channel Port (Digital)});"
        },
        "Potentiometer" : {
	    "Defaults": ["AnalogInput", "All"],
	    "ClassName": "AnalogChannel"
        },
        "Limit Switch" : {
	    "Defaults": ["DigitalInput", "All"],
	    "ClassName": "DigitalInput"
        },
        "Victor" : {
	    "Defaults": ["PWMOutput", "All"],
	    "ClassName": "Victor"
        },
        "Jaguar" : {
	    "Defaults": ["PWMOutput", "All"],
	    "ClassName": "Jaguar"
        }
    }
}
