{
    "Name": "Java",
    "Type": "Code",
    "Toolbar": true,
    "New Files": "files.txt",
    "Modifie Files": "files.txt",
    "Vars": [
	{"Name": "project",
	 "Value": "${robot.getProperty('Java Project')}"},
	{"Name": "package",
	 "Value": "${robot.getProperty('Java Package')}"},
	{"Name": "package-path",
	 "Value": "${project}/src/${package.replace('.', '/')}"}
    ],
    "Defaults": {
	"Component": {
	    "Export": "RobotMap",
	    "Import": "import edu.wpi.first.wpilibj.*;",
	    "Declaration": "public static ${ClassName} ${helper.constantName($Name)};",
	    "Subsystem Export": "${ClassName} ${Short_Name} = RobotMap.${helper.constantName($Name)};"
	},
	"None": {
	    "Export": "",
	    "Import": "",
	    "Declaration": "",
	    "Construction": "",
	    "Extra": "",
	    "Subsystem Export": "",
	    "ClassName": "",
	    "Template": ""
	},
	"AnalogInput": {
	    "Construction": "${helper.constantName($Name)} = new ${ClassName}(${Input_Module_Analog}, ${Input_Channel_Analog});"
	},
	"DigitalInput": {
	    "Construction": "${helper.constantName($Name)} = new ${ClassName}(${Input_Module_Digital}, ${Input_Channel_Digital});"
	},
	"PWMOutput": {
	    "Construction": "${helper.constantName($Name)} = new ${ClassName}(${Output_Module_Digital}, ${Output_Channel_PWM});"
	},
	"OI": {
	    "Export": "OI",
	    "Import": "import edu.wpi.first.wpilibj.*;",
	    "Declaration": "public ${ClassName} ${helper.variableName($Name)};"
	},
	"Button": {
	    "Import": "import edu.wpi.first.wpilibj.buttons.*;",
	    "Extra":  "${helper.variableName($Name)}.${When_to_Run}(new ${helper.className($Command)}());"
	},
	"Command": {
	    "Export": "",
	    "Import": "import ${package}.commands.*;"
	}
    },
    "Instructions": {
	"Robot" : {
	    "Defaults": ["None"],
	},
	"Subsystems" : {
	    "Defaults": ["None"],
	},
	"OI" : {
	    "Defaults": ["None"],
	},
	"Commands" : {
	    "Defaults": ["None"],
	},
	
	"Subsystem" : {
	    "Defaults": ["None"],
	    "Export": "CommandBase",
	    "Import": "import ${package}.subsystems.*;",
	    "Construction": "public static ${helper.className($Short_Name)} ${helper.variableName($Short_Name)} = new ${helper.className($Short_Name)}();"
	},
	
	"PID Controller" : {
	    "Defaults": ["Component", "None"],
	    "ClassName": "SendablePIDController",
	    "Import": "import edu.wpi.first.wpilibj.smartdashboard.*;",
	    "Construction": "${helper.constantName($Name)} = new ${ClassName}(${P}, ${I}, ${D}, ${F}, ${helper.constantName($Input)}, ${helper.constantName($Output)}, ${Period});",
	    "Extra": "${helper.constantName($Name)}.setContinuous(${Continuous});
        #if (${Limit_Input})${helper.constantName($Name)}.setInputRange(${Minimum_Input}, ${Maximum_Input});#end
        #if (${Limit_Output})${helper.constantName($Name)}.setInputRange(${Minimum_Output}, ${Maximum_Output});#end
        #if (${Send_to_SmartDashboard})SmartDashboard.putData(\"${Name}\", ${helper.constantName($Name)});#end"
	},
	"Robot Drive 2" : {
	    "Defaults": ["Component", "None"],
	    "ClassName": "RobotDrive",
	    "Construction": "${helper.constantName($Name)} = new ${ClassName}(${helper.constantName($Left_Motor)}, ${helper.constantName($Right_Motor)});"
	},
	"Robot Drive 4" : {
	    "Defaults": ["Component", "None"],
	    "ClassName": "RobotDrive",
	    "Construction": "${helper.constantName($Name)} = new ${ClassName}(${helper.constantName($Left_Front_Motor)}, ${helper.constantName($Left_Rear_Motor)}
        ${helper.constantName($Right_Front_Motor)}, ${helper.constantName($Right_Rear_Motor)});"
	},
	
        "Gyro" : {
	    "Defaults": ["AnalogInput", "Component", "None"],
	    "ClassName": "Gyro",
	    "Extra": "${helper.constantName($Name)}.setSensitivity(${Sensitivity});"
        },
        "Accelerometer" : {
	    "Defaults": ["AnalogInput", "Component", "None"],
	    "ClassName": "Accelerometer"
        },
        "Quadrature Encoder" : {
	    "Defaults": ["DigitalInput", "Component", "None"],
	    "ClassName": "Encoder",
	    "Construction": "${helper.constantName($Name)} = new ${ClassName}(${Channel_A_Module_Digital}, ${Channel_A_Channel_Digital}, ${Channel_B_Module_Digital}, ${Channel_B_Channel_Digital});"
        },
        "Potentiometer" : {
	    "Defaults": ["AnalogInput", "Component", "None"],
	    "ClassName": "AnalogChannel"
        },
        "Limit Switch" : {
	    "Defaults": ["DigitalInput", "Component", "None"],
	    "ClassName": "DigitalInput"
        },
	
        "Victor" : {
	    "Defaults": ["PWMOutput", "Component", "None"],
	    "ClassName": "Victor"
        },
        "Jaguar" : {
	    "Defaults": ["PWMOutput", "Component", "None"],
	    "ClassName": "Jaguar"
        },
	
	"Joystick" : {
	    "Defaults": ["OI", "None"],
	    "ClassName": "Joystick",
	    "Construction": "${helper.variableName($Name)} = new ${ClassName}(${Number});"
        },
	"Joystick Button" : {
	    "Defaults": ["Button", "OI", "None"],
	    "ClassName": "JoystickButton",
	    "Construction": "${helper.variableName($Name)} = new ${ClassName}(${helper.variableName($Joystick)}, ${Button});"
        },
	
	"Command" : {
	    "Defaults": ["Command", "None"],
	    "Template": "Command.java"
        },
	"Command Group" : {
	    "Defaults": ["Command", "None"],
	    "Template": "CommandGroup.java"
        }
    }
}
