{
    "Defaults": {
	"All": {
	    "Import": "import edu.wpi.first.wpilibj.${ClassName};",
	    "Declaration": "public static ${ClassName} ${Name};",
	    "Extra": "",
	    "Subsystem Export": "${ClassName} ${Short Name} = RobotMap.${Name};"
	},
	"None": {
	    "Import": "",
	    "Declaration": "",
	    "Construction": "",
	    "Extra": "",
	    "Subsystem Export": ""
	},
	"AnalogInput": {
	    "Construction": "${Name} = new ${ClassName}(${Input Module (Analog)}, ${Input Channel (Analog)});"
	},
	"DigitalInput": {
	    "Construction": "${Name} = new ${ClassName}(${Input Module (Digital)}, ${Input Channel (Digital)});"
	},
	"PWMOutput": {
	    "Construction": "${Name} = new ${ClassName}(${Output Module (Digital)}, ${Output Channel (PWM)});"
	}
    },
    "Palette": {
	"Robot" : {
	    "Defaults": ["None"],
	    "ClassName": ""
	},
	"Subsystem" : {
	    "Defaults": ["None"],
	    "ClassName": ""
	},
	"PID Controller" : {
	    "Defaults": ["All"],
	    "ClassName": "SendablePIDController",
	    "Import": "import edu.wpi.first.wpilibj.smartdashboard.*;",
	    "Construction": "${Name} = new ${ClassName}(${P}, ${I}, ${D}, ${F}, ${Input}, ${Output}, ${Period});",
	    "Extra": "${Name}.setContinuous(${Continuous});\n        if (${Limit Input}) ${Name}.setInputRange(${Minimum Input}, ${Maximum Input});\n        if (${Limit Output}) ${Name}.setInputRange(${Minimum Output}, ${Maximum Output});\n        if(${Send to SmartDashboard}) SmartDashboard.putData(\"${Name}\", ${Name});"
	},
	"Robot Drive 2" : {
	    "Defaults": ["All"],
	    "ClassName": "RobotDrive",
	    "Construction": "${Name} = new ${ClassName}(${Left Motor}, ${Right Motor});"
	},
	"Robot Drive 4" : {
	    "Defaults": ["All"],
	    "ClassName": "RobotDrive",
	    "Construction": "${Name} = new ${ClassName}(${Left Front Motor}, ${Left Rear Motor}, ${Right Front Motor}, ${Right Rear Motor});"
	},
        "Gyro" : {
	    "Defaults": ["AnalogInput", "All"],
	    "ClassName": "Gyro",
	    "Extra": "${Name}.setSensitivity(${Sensitivity});"
        },
        "Accelerometer" : {
	    "Defaults": ["AnalogInput", "All"],
	    "ClassName": "Accelerometer"
        },
        "Quadrature Encoder" : {
	    "Defaults": ["DigitalInput", "All"],
	    "ClassName": "Encoder",
	    "Construction": "${Name} = new ${ClassName}(${Channel A Module (Digital)}, ${Channel A Channel (Digital)}, ${Channel B Module (Digital)}, ${Channel B Channel (Digital)});"
        },
        "Potentiometer" : {
	    "Defaults": ["AnalogInput", "All"],
	    "ClassName": "AnalogChannel"
        },
        "Limit Switch" : {
	    "Defaults": ["DigitalInput", "All"],
	    "ClassName": "DigitalInput"
        },
        "Victor" : {
	    "Defaults": ["PWMOutput", "All"],
	    "ClassName": "Victor"
        },
        "Jaguar" : {
	    "Defaults": ["PWMOutput", "All"],
	    "ClassName": "Jaguar"
        }
    }
}
